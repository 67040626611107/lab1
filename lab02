#include <unistd.h> 
#include <stdio.h> 
int main () {
	int digit;
	int digit2;

	printf("n1=");
	scanf("%d",&digit);
	printf("n2=");
	scanf("%d",&digit2);
	if (digit > digit2) {
	    printf("N1 > N2\n");
	    printf("%d",digit);
	} else if (digit < digit2) {
	   printf("N1 < N2 \n");
	   printf("%d",digit);
	}
	return (0);
};
